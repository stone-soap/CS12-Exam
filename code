 			//INIT - GENERATE RANDOM NUMBER
			const COMPUTER_GUESS = Math.ceil( Math.random() * 130 );
			console.log(COMPUTER_GUESS);


		//THIS IS NOT WORKING?? 		- if statements refuse to run when animation is on. idk why??
			//PROCESS - IF STATEMENTS 
			var nutProcess = function(){
				if ( COMPUTER_GUESS >= 120 ) {
					console.log('the special nut :3');
						specialNut();

				} else {
					console.log('loser :P');
						regularNut();
				}
			}

			//OUTPUT 1 - SPECIAL
			var specialNut = function(){
				console.log("Good job!! You're very lucky :)");
			}


			//OUTPUT 2 - REGULAR
			var regularNut = function(){
				console.log("Keep trying, I believe in you :)");
			}

			document.body.onload = nutProcess




