	//	PROCESS -	FOR LOOP
			nutObj.showNuts = function(){ //LOOP THROUGH THE ARRAY TO DISPLAY ALL THE NUTS ON SCREEN
					//console.log('help');
				var nutDisplayCode   = ""; //define and empty string 

					for (i in this.goldenWalnutArr) { //i = row
 						var newImage = document.createElement( "img" );

 							newImage.src = "https://stardewvalleywiki.com/mediawiki/images/5/54/Golden_Walnut.png";

 						
 						var topLocation = nutObj.goldenWalnutArr[ i ].top + 'px';
						var leftLocation = nutObj.goldenWalnutArr[ i ].left + 'px';
							//console.log('nuts? ' + top);
						newImage.id = 'walnut' + [i];
							//console.log(newImage.style.id);
						newImage.style.top = topLocation;
 						newImage.style.left = leftLocation;
 						nutObj.outputNuts(newImage);

 					}
 			}



 			console.log("my favourite npc in this entire game lives on the island. His name is LEO and he is a little kid who lives in a tree hut with PARROTS. he is happy when you gift him MANGOES. he's the best.")

 		//OUTPUT??

 				nutObj.outputNuts = function ( newImage ) {  
			   		//console.log(newImage.id);
			   		document.querySelector( '#outputId' ).appendChild(newImage);

				}
				
				nutObj.nutInitMain = function(){
					nutObj.createNutArr();
					nutObj.showNuts();
					nutObj.animateNuts();
				}

				nutObj.animateNuts = function(){
					//console.log('animate the nuts: ');
					for (i = 0; i < nutObj.goldenWalnutArr.length; i++) { //i = row
						$( '#' + 'walnut' + i ).animate( { width: "+=6px", left: "-=3px", height: "+=6px", top: "-=3px"}, { duration: "slow" }  ); 
						$( '#' + 'walnut' + i ).animate( { width: "-=6px", left: "+=3px", height: "-=6px", top: "+=3px"}, { duration: "slow" }  ); 

					//console.log('nutObj.goldenWalnutArr.length');
					}
					setInterval( nutObj.animateNuts, 1000);
				} 



				document.body.onload = nutObj.nutInitMain;
			//document.body.onload = nutObj.animateNuts;
				
				
