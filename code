			var nutToStardrop = function (){
				console.log('stardrop :3');
				document.querySelector('#nut').src="https://stardewvalleywiki.com/mediawiki/images/a/a5/Stardrop.png";
			}
			var stardropToNut = function (){
				console.log('back to nut :3');
				document.querySelector('#nut').src="https://stardewvalleywiki.com/mediawiki/images/5/54/Golden_Walnut.png";
			}


			document.querySelector('#nut').onmouseover = nutToStardrop;
			document.querySelector('#nut').onmouseout = stardropToNut;


